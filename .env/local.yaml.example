env: "local"
http_server:
  host: "0.0.0.0"
  port: ${APP_PORT_CONTAINER}
  timeout: 5s
  cors_origins:
    - "http://localhost:8082"
    - "http://127.0.0.1:8082"
postgres:
  host: "postgres"
  port: ${PG_PORT_CONTAINER}
  user: ${POSTGRES_USER}
  password: ${POSTGRES_PASSWORD}
  db: ${POSTGRES_DB}
  sslmode: "disable"
